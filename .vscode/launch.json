{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "QBO OAuth Server",
      "type": "debugpy",
      "request": "launch",
      "module": "firm_qbo.auth.oauth_server",
      "justMyCode": true
    },
    {
      "name": "Clio OAuth Server",
      "type": "debugpy",
      "request": "launch",
      "module": "firm_clio.auth.oauth_server",
      "justMyCode": true
    },
    {
      "name": "üêç Flask: Run App",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/firm_clio/auth/app.py",
      "env": {
        "FLASK_APP": "app.py",
        "FLASK_ENV": "development"
      },
      "args": [],
      "jinja": true,
      "console": "integratedTerminal"
    },
    {
      "name": "ü™õ Debug load_composite_matter",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/debug_matter.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}